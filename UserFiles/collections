[{"name": "Order Header and Line for SO Mass Update", "date": "December 19, 2022", "sql": "SELECT h.source_order_id\n     , h.SOURCE_ORDER_NUMBER\n     , h.order_number\n     , h.CHANGE_VERSION_NUMBER\n     , h.SUBMITTED_DATE\n     , l.LINE_NUMBER\n     , l.inventory_item_id LINE_ITEM_ID\n     , i.item_number LINE_ITEM_NUMBER\n     , f.inventory_item_id FULFILL_ITEM_ID\n     , if.item_number FULFILL_ITEM_NUMBER\n     , l.ORDERED_QTY\n     , h.last_update_date\nFROM doo_headers_all h\n   , doo_lines_all l\n   , doo_fulfill_lines_all f\n   , egp_system_items_vl i\n   , egp_system_items_vl IF\nWHERE h.SOURCE_ORDER_NUMBER ='10151841'\n  AND h.submitted_flag='Y'\n  AND h.header_id=l.header_id\n  AND h.header_id=f.header_id\n  AND l.line_id=f.line_id\n  AND i.inventory_item_id=l.inventory_item_id\n  AND i.organization_id=l.INVENTORY_ORGANIZATION_ID\n  AND if.inventory_item_id=f.inventory_item_id\n  AND if.organization_id=f.INVENTORY_ORGANIZATION_ID\nORDER BY to_number(l.LINE_NUMBER)"}]